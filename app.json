{
  "expo": {
    "name": "eshramik-mobile",
    "slug": "eshramik-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "splash": {
      "image": "./assets/eshramik.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.mobile.eshramik",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mobile.eshramik",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
